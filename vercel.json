{
  "functions": {
    "api/index.py": {
      "runtime": "python3.12"
    },
    "api/**/*.py": {
      "runtime": "python3.12"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    }
  ]
}