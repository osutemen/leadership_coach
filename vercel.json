{
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 60
    },
    "api/**/*.py": {
      "runtime": "python3.11"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "env": {
    "PYTHONPATH": "./api"
  }
}